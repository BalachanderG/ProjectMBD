<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jdo PUBLIC
    "-//Sun Microsystems, Inc.//DTD Java Data Objects Metadata 2.0//EN"
    "http://java.sun.com/dtd/jdo_2_0.dtd">
<jdo>
    <package name="in.co.blackphoenix.shexp.jdo">
        <class
              name="PMF">
        </class>
        <class
              name="Bill">
            <field
                  name="id"
                  persistence-modifier="persistent"/>
            <field
                  name="description"
                  persistence-modifier="persistent"/>
            <field
                  name="amount"
                  persistence-modifier="persistent"/>
            <field
                  name="numberOfParticipants"
                  persistence-modifier="persistent"/>
            <field
                  name="myParticipants"
                  persistence-modifier="persistent"/>
        </class>
        <class
              name="BalanceSheet">
        </class>
        <class
              name="PerUserTransaction">
            <field
                  name="myName"
                  persistence-modifier="persistent"/>
            <field
                  name="myPart"
                  persistence-modifier="persistent"/>
        </class>
    </package>
</jdo>